{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es2019",
      "esnext.asynciterable"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "strict": true,
    "newLine": "LF",
    "resolveJsonModule": true,
    "noLib": false,
    "skipLibCheck": true,
    "declaration": true,
    "removeComments": true,
    "strictPropertyInitialization": false,
    "sourceMap": true
  }
}
